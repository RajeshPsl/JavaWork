
public class Demo {
	
	
	public static void main (String[] args)
	{	
		//System.out.print("Hello");	
		Compare2Arrays();
	}

public static void Compare2Arrays()
{
	int arr1[] = {1, 2, 4};
    int arr2[] = {1, 2, 3};
    if (arr1 == arr2) // Same as arr1.equals(arr2)
        System.out.println("Same");
    else
        System.out.println("Not same");
	
}


}